{J{js/objs/cube.js}J}

<script>
	jQuery(
		function() {
			var camera = new Camera3D(),
				item = new Object3D($("#item")),
				scene = new Scene3D();

			item.addChild(new Sphere(100));
			camera.init(0,0,0,280);
			scene.addToScene(item);
			
			var animateIt = function(){
				axisRotation.y += .01;
				axisRotation.x += .002;
				axisRotation.z += .02;

				scene.renderCamera(camera);
			};

			setInterval(animateIt, 20);

			jQuery('li').fadeIn();
		}
	);

</script>
